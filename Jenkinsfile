pipeline{
    agent any
    stages{
        stage('build'){
            steps{  
          echo "Welcome to Jenkins Environment"
            sh'echo using shell within Jenkinsfile'
            echo 'not using shell within Jenkinsfile'
            echo 'Hello World'
            echo 'Hello everyone'
            echo 'Hi there!!!!'
           }
       }
        stage('test'){
            steps{  
          echo "Welcome to Jenkins Environment"
            sh'echo using shell within Jenkinsfile'
            echo 'not using shell within Jenkinsfile'
           echo 'Goood Morning!'
           }
       }
    }
}
